//fiil():- Sets the color used to fill shapes. For example, if you run fill(204, 102, 0), 
//all subsequent shapes will be filled with orange. 

//6.Refine

void setup() {
  size(400, 400);
}

void draw() {
  background(255);  // Set the background to white
  fill(153);        // Set the fill color to gray
  rect(120, 80, 220, 220);  // Draw a gray rectangle
}
